    "pako": "^1.0.6",
    "post-robot": "^8.0.28",
    "qs": "^6.5.2",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.2.0",
    "bootstrap": "^4.1.3",
    "copy-webpack-plugin": "^4.5.2",
    "normalize-newline-cli": "^1.1.0",
    "serverless-plugin-cloudfront-lambda-edge": "^2.0.0",
    "serverless-s3-sync": "^1.7.0",
    "serverless-stack-output": "^0.2.3",
    "ssri": "^6.0.0",
    "worker-loader": "^2.0.0"
